@model Gerenciamento_de_Horas.Models.LancamentoHora.Lancamentos

@{
    ViewData["Title"] = "Delete";
}


<h3>Deseja realmente excluir ?</h3>
<div>
    <h4>Lançamentos</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Desenvolvedor)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Desenvolvedor.Id)
        </dd class>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataInicio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataInicio)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataFim)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataFim)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Excluir" class="btn btn-danger" /> |
        <a  class="btn btn-dark"  asp-action="Index">Voltar</a>
    </form>
</div>
