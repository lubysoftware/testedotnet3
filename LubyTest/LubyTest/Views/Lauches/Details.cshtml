@model LubyTest.Models.Lauch

@{
    ViewData["Title"] = "Detalhes do Lançamento";
}

<h4 style="padding-top: 50px">@ViewData["Title"]</h4>
<hr />

<div class="col-md-6">
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DeveloperId)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Developer.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Project.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Project.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StarDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StarDate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.EndDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EndDate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Hours)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Hours)
        </dd>

    </dl>
</div>
<div>
    <a class="btn btn-info" asp-action="Index"> << Voltar</a>
</div>
